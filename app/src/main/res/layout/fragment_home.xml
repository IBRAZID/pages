<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".HomeFragment">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <Button
            android:id="@+id/button3"
            android:layout_width="188dp"
            android:layout_height="67dp"
            android:backgroundTint="#03A9F4"
            android:text="Water"
            tools:layout_editor_absoluteX="112dp"
            tools:layout_editor_absoluteY="191dp" />

        <Button
            android:id="@+id/button5"
            android:layout_width="188dp"
            android:layout_height="67dp"
            android:backgroundTint="#03A9F4"
            android:text="Israeli Cans 330ml"
            tools:layout_editor_absoluteX="111dp"
            tools:layout_editor_absoluteY="332dp" />

        <TextView
            android:id="@+id/textView8"
            android:layout_width="match_parent"
            android:layout_height="2dp"
            android:layout_marginBottom="74dp"
            android:background="@color/cardview_dark_background"
            android:backgroundTint="#03A9F4"
            app:layout_constraintBottom_toTopOf="@+id/button3"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView6" />

        <Button
            android:id="@+id/button4"
            android:layout_width="188dp"
            android:layout_height="67dp"
            android:backgroundTint="#03A9F4"
            android:text="Arabic Cans 330ml"
            tools:layout_editor_absoluteX="111dp"
            tools:layout_editor_absoluteY="482dp" />

        <TextView
            android:id="@+id/textView6"
            android:layout_width="match_parent"
            android:layout_height="65dp"
            android:backgroundTint="@color/mainBlue"
            android:gravity="center_vertical"
            android:outlineAmbientShadowColor="@color/mainBlue"
            android:outlineProvider="bounds"
            android:outlineSpotShadowColor="@color/mainBlue"
            android:text="Home"
            android:textAlignment="center"
            android:textSize="30dp"
            tools:layout_editor_absoluteX="0dp"
            tools:layout_editor_absoluteY="50dp" />

        <TextView
            android:id="@+id/textView7"
            android:layout_width="match_parent"
            android:layout_height="48dp"
            android:background="@color/cardview_dark_background"
            android:backgroundTint="#03A9F4"
            android:gravity="center_vertical"
            android:text="  LaLuna Universe "
            android:textColor="@color/white"
            android:textSize="23dp"
            tools:layout_editor_absoluteX="1dp"
            tools:layout_editor_absoluteY="-1dp" />

        <TextView
            android:id="@+id/textView9"
            android:layout_width="match_parent"
            android:layout_height="67dp"
            android:background="@color/cardview_dark_background"
            android:backgroundTint="#03A9F4"
            android:gravity="center_vertical"
            android:text="Thanks For Chosing LaLuna"
            android:textAlignment="center"
            android:textColor="@color/white"
            android:textSize="20dp"
            tools:layout_editor_absoluteX="0dp"
            tools:layout_editor_absoluteY="665dp" />

        <Button
            android:id="@+id/button6"
            android:layout_width="93dp"
            android:layout_height="48dp"
            android:backgroundTint="@color/mainBlue"
            android:text="Cart"
            android:textSize="17dp"
            tools:layout_editor_absoluteX="318dp"
            tools:layout_editor_absoluteY="0dp" />


    </androidx.constraintlayout.widget.ConstraintLayout>
</FrameLayout>